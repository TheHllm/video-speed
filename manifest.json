{
    "name": "Video speed",
    "version": "1.0",
    "description": "Let's you change the speed of any video.",
    "manifest_version": 2,
    "icons":{
        "128": "/assets/icons/icon_128.png"
    },
    "browser_action": {
        "default_icon": "/assets/icons/icon.png",
        "default_popup": "index.html"
    },
    "permissions": ["activeTab", "tabs", "https://*/", "http://*/"],
    "background": {
        "page": "background.html",
        "persistent": false
    }

}